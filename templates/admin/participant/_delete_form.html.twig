<form method="post" action="{{ path('app_admin_participant_delete', {'id': participant.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer cet utilisateur?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ participant.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
